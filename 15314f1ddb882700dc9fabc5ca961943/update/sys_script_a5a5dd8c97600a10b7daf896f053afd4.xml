        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {
	var details = {
		"key": current.getValue("key").trim(),
		"display_name_singular": current.getValue("display_name_singular").trim(),
		"display_name_plural": current.getValue("display_name_plural").trim(),
		"label_type_initial": current.getValue("label_type_initial").trim(),
		"illumio_pce_configuration": current.getValue("illumio_pce_configuration"),
		"href": current.getValue("href").trim()
	};
	
	if (gs.nil(current.getValue("href"))) {
		gs.error("The href for label dimension " + current.getValue("key") + " is not present. Hence, it couldn't be updated.");
	} else {
		var IlUtils = new x_illu2_illumio.IllumioUtils();
		IlUtils.updateLabelDimension(details);
	}
})(current, previous);]]></script>
